package Chapter2;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N  = sc.nextInt();
		int[][] arr = new int[N][N];
		int count;
		for(int i=0; i<N; i++) {
			for(int j=0; j<N; i++) {
				arr[i][j] =sc.nextInt();
				}	
		}
		for(int i=0; i<N; i++) {
			for(int j=0; j<N; i++) {
				if(arr[i][j]>arr[i][j+1] && arr[i][j]>arr[i][j-1] && arr[i][j]> arr[i+1][j]
					&&	arr[i][j]>arr[i-1][j]) {
					count++;
				}
				}	
		}
		System.out.println(count);
	}
}

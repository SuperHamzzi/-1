package Chapter3;

import java.util.Scanner;

public class Main {
	public int solution(int n, int m, int[] a) {
		int answer =0;
		int sum =0;
		for(int i=0;i<n-2;i++) {
			for(int j=i;j<i+m;j++) {
				sum += a[j];
			}
			if(sum>answer) {
				answer =sum;
			}
		}
		
		
		return answer;
	}
	

	public static void main(String[] args) {
		Main T =new Main();
		Scanner sc =new Scanner(System.in);
		int N =sc.nextInt();
		int M =sc.nextInt();
		int[] a =new int[N];
		for(int i=0;i<N;i++) {
			a[i] = sc.nextInt();
		}
		System.out.println(T.solution(N,M,a));
		

	}

	

}

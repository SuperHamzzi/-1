package Chapter2;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int arr[][] = new int[5][N];
		int count =0;
		int num;
		for(int i =1; i<N+1; i++) {
			for(int j =1; i<N+1; j++) {
				arr[i][j] =sc.nextInt();
		
			}
		}
		for(int i =1; i<N+1; i++) {
			for(int j =1; i<N+1; j++) {
				for(int k =1; k<N+1; k++) {
					if(arr[i][k]==arr[j][k]){
						count++;
						break;
					}
				}
		
			}
		}
		System.out.println(count);
		
		
		
		
		
	}
}

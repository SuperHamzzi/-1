package Chapter2;

import java.util.ArrayList;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		int N = sc.nextInt();
		int count = 1;
		int[] arr = new int[N];
		for(int i=0; i<N; i++) {
			int a = sc.nextInt();
			arr[i] = a;
		}
		for(int i=0; i<N; i++) {
			for(int j=0; j<N; j++) {
				if(i==j) {
					}else if(i<j){
						count++;
					}
				arr[i] = count;
			}
		}
		System.out.println(arr);
	}
	
}

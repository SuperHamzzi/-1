package Chapter2;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
			int N =sc.nextInt();
			
			String[] arr = new String[N];
			for(int i=1; i<=N; i++) {
				if(i==1 && i==0) {
					arr[i]=1;}else {
				arr[i]=arr[i-1]+arr[i-2];
					}
				}
			System.out.println(arr);
			
	}
	}


